<section class="pagination">
    <% if(hasPreviousPage){ %>
        <a href="?page=<%= previousPage %>"> < </a>
    <% } %>
    <% if(currentPage != 1){ %>
        <a href="?page=1">1</a>
    <% } %>
        <a href="?page=<%= currentPage %>" class="active"><%= currentPage %></a>
    <% if(hasNextPage){ %>
        <a href="?page=<%= nextPage %>"> > </a>
    <% } %> 
    <% if(lastPage != currentPage){ %>
        <a href="?page=<%= lastPage %>"> Last Page </a>
    <% } %> 
</section>